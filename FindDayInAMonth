a=int(input("Enter any date:"))
if (a<=31) and (a>0):
    print("Valid")
    if(a%7==0):
        print("Sunday")
    elif(a%7==1):
        print("Monday")
    elif(a%7==2):
        print("Tuesday")
    elif(a%7==3):
        print("Wednesday")
    elif(a%7==4):
        print("Thursday")
    elif(a%7==5):
        print("Friday")
    elif(a%7==6):
        print("Saturday")
else:
    print("not valid date")
